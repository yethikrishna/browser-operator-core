{
  "name": "browser-operator-core",
  "version": "1.0.0",
  "description": "AI-powered web automation core - extracted from Browser Operator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./headless": {
      "import": "./dist/headless.mjs",
      "require": "./dist/headless.js",
      "types": "./dist/headless.d.ts"
    },
    "./react": {
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "rollup -c rollup.config.js --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "automation",
    "web",
    "browser",
    "agent",
    "llm",
    "devtools",
    "chrome"
  ],
  "author": "Browser Operator Contributors",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tysonthomas9/browser-operator-devtools-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/tysonthomas9/browser-operator-devtools-frontend/issues"
  },
  "homepage": "https://github.com/tysonthomas9/browser-operator-devtools-frontend#readme",
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.9.6",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "eventemitter3": "^5.0.1"
  }
}